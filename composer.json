{
    "name": "nadialabs/nadia-simple-security-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "",
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.12|^2.0",
        "doctrine/orm": "^2.6",
        "symfony/console": "^3.0|^4.0",
        "symfony/dependency-injection": "^3.0|^4.0",
        "symfony/form": "^3.0|^4.0",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/security-bundle": "^3.0|^4.0",
        "symfony/twig-bundle": "^3.0|^4.0",
        "twig/twig": "^2.0|^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "symfony/phpunit-bridge": "^3.4.31|^4.3.4",
        "symfony/yaml": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Nadia\\Bundle\\NadiaSimpleSecurityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nadia\\Bundle\\NadiaSimpleSecurityBundle\\Tests\\": "tests/"
