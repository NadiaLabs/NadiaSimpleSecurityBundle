{
    "name": "nadialabs/nadia-simple-security-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/security-bundle": "^3.0|^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "symfony/phpunit-bridge": "~3.3|^4.0",
        "symfony/yaml": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Nadia\\Bundle\\NadiaSimpleSecurityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nadia\\Bundle\\NadiaSimpleSecurityBundle\\Tests\\": "tests/"
        }
    }
}

